Leetcode :- 3223. Minimum Length of String After Operations

int minimumLength(string s) {
        unordered_map<char,int> m;
        for(auto x:s){
            m[x]++;
        }
        int ans=0;
        for(auto x:m){
            if(x.second<3) ans+=x.second;
            else{
                if(x.second%2!=0) ans+=1;
                else ans+=2;
            }
        }
        return ans;
}



